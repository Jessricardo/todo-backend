{
  "name": "user",
  "plural": "users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "updatedAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "lists": {
      "type": "hasMany",
      "model": "list",
      "foreignKey": ""
    },
    "jwts": {
      "type": "hasMany",
      "model": "jwt",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
